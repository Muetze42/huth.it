{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "homepage": "https://huth.it",
    "authors": [
        {
            "name": "Norman Huth",
            "email": "norman@huth.it"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Muetze42/laravel-trash-mail"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "dmitrybubyakin/nova-medialibrary-field": "^3.2.11",
        "fruitcake/laravel-cors": "^2.0.4",
        "guzzlehttp/guzzle": "^7.3.0",
        "hisorange/browser-detect": "^4.4",
        "inertiajs/inertia-laravel": "^0.4.4",
        "laravel-notification-channels/telegram": "^0.5.1",
        "laravel/framework": "^8.57",
        "laravel/nova": "^3.29",
        "laravel/socialite": "^5.2.4",
        "laravel/tinker": "^2.6.1",
        "laravelwebdev/responsive": "^1.0.1",
        "muetze/nova-icon-select": "^1.0",
        "muetze42/nova-charcounted-fields": "^1.0",
        "norman-huth/laravel-trash-mail": "dev-main",
        "norman-huth/muetze-site": "^0.0.6",
        "optimistdigital/nova-sortable": "^2.4",
        "scssphp/scssphp": "^1.6",
        "socialiteproviders/github": "^4.1",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-activitylog": "^4.1.1",
        "spatie/laravel-medialibrary": "9.0.0",
        "tightenco/ziggy": "^1.4",
        "timothyasp/nova-color-field": "^1.0.4",
        "titasgailius/search-relations": "^2.0.1",
        "ext-dom": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.10.1",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.9",
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
